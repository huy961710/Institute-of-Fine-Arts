@model IEnumerable<Student_Design_Posting.Models.Competition>

@{
    ViewData["Title"] = "InCommingCompetition";
}

<h1>InCommingCompetition</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CompetitionName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StaffId)
            </th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CompetitionName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StaffId)
            </td>
            <td>
                @Html.ActionLink("Register", "Upload", new { id = item.CompetitionId })
            </td>
            <td>
                @if (TempData["testmsgs"] != null)
                {
                    @Html.Raw(TempData["testmsgs"])
                }
            </td>
        </tr>
}
    </tbody>
</table>
<div>
    @Html.ActionLink("Back to List", "Index", "StudentDesignPosting")
</div>
